          "parameters": [
            {
              "in": "path",
              "name": "delay_value",
              "required": true,
              "schema": {
                "type": "integer",
                "minimum": 0,
                "maximum": 10
              },
              "description": "The delay in seconds"
            }
          ],
          "responses": {
            "200": {
              "description": "Returns a delayed response"
            }
          },
          "summary": "Returns a delayed response (max of 10 seconds).",
          "tags": [
            "Delayed Response",
            "Path"
          ]
        }
      },
      "/client2/CICDIngresshttpbin/status/{codes}": {
        "get": {
